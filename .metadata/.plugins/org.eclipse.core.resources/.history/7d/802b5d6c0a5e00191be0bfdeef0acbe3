package aula02;

public class TestaFuncionario {
	public static void main(String[] args) {
		Funcionario f1 = mre Funcionario();
		f1.nome = "Andre";
		f1.salario = 1000;
		f1.status = "Professor";
		System.err.println(f1.salario);
		
		f1.calculoBonificacao(50);
		System.err.println(f1.salario);
	}
