package aula02;

public class Gerente extends Funcionario{
	int senha;
	
	double getBonus() {
		return this.salario * 0.25;
	}
		
	public boolean verificaSenha(int senha) {
		if (this.senha == senha) {
			System.out.println("Acesso autorizado");
			return true;			
		} else {
			System.err.println("Acesso negado");
			return false;
		}
		
	}
}
